{
  "name": "japan-train-data",
  "version": "0.0.1",
  "description": "Scripts to generate train data for Japan, with translations, and an npm package for convenience",
  "author": "Marco LÃ¼thy <marco@luethy.net>",
  "keywords": [
    "japan",
    "train",
    "data"
  ],
  "main": "lib/index.js",
  "repository": "git@github.com:adieuadieu/japan-train-data.git",
  "bugs": {
    "url": "https://github.com/adieuadieu/serverless-chrome/issues"
  },
  "homepage": "https://github.com/adieuadieu/serverless-chrome",
  "license": "MIT",
  "engines": {
    "node": ">= 6.10.0"
  },
  "config": {
    "jsSrc": "src/"
  },
  "scripts": {
    "test": "npm run lint && nyc ava",
    "watch:test": "ava --watch",
    "ava": "ava",
    "lint": "npm run lint:eslint -s",
    "lint:eslint": "eslint $npm_package_config_jsSrc",
    "coverage": "nyc report"
  },
  "dependencies": {
    "@google-cloud/translate": "^0.8.1",
    "awaiting": "^2.2.0",
    "dotenv": "^4.0.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "codacy-coverage": "^2.0.2",
    "coveralls": "^2.13.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-tap": "^2.0.0",
    "nyc": "^10.2.0"
  },
  "ava": {
      "require": "babel-register",
      "babel": "inherit"
    },
  "babel": {
    "sourceMaps": "inline",
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6.1
          }
        }
      ],
      "stage-3"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "ava",
      "import"
    ],
    "extends": [
      "airbnb-base",
      "plugin:ava/recommended"
    ],
    "settings": {
      "import/parser": "babel-eslint",
      "import/resolve": {
        "moduleDirectory": [
          "node_modules",
          "src",
          "./"
        ]
      }
    },
    "rules": {
      "no-console": 0,
      "semi": [
        "error",
        "never"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "space-before-function-paren": [
        "error",
        "always"
      ]
    }
  }
}
